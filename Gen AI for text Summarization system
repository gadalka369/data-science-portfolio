      "cell_type": "code",
      "source": [
        "# ============================================================================\n",
        "# STEP 4: ANALYSIS & VISUALIZATION\n",
        "# ============================================================================\n",
        "\n",
        "print(\"[4/5] Analyzing Summarization Performance...\")\n",
        "\n",
        "# Create DataFrame\n",
        "stats_df = pd.DataFrame(summary_stats)\n",
        "print(\"\\n--- Summarization Statistics ---\")\n",
        "print(stats_df)\n",
        "\n",
        "# Calculate average metrics\n",
        "avg_compression = stats_df['Compression_Ratio'].mean()\n",
        "avg_summary_length = stats_df['Summary_Words'].mean()\n",
        "\n",
        "print(f\"\\nAverage Compression Ratio: {avg_compression:.1f}%\")\n",
        "print(f\"Average Summary Length: {avg_summary_length:.1f} words\")\n",
        "\n",
        "# Visualizations\n",
        "fig, axes = plt.subplots(2, 2, figsize=(14, 10))\n",
        "\n",
        "# Plot 1: Word Count Comparison\n",
        "x_pos = np.arange(len(stats_df))\n",
        "width = 0.35\n",
        "\n",
        "axes[0, 0].bar(x_pos - width/2, stats_df['Original_Words'], width,\n",
        "               label='Original', color='steelblue', alpha=0.8)\n",
        "axes[0, 0].bar(x_pos + width/2, stats_df['Summary_Words'], width,\n",
        "               label='Summary', color='coral', alpha=0.8)\n",
        "axes[0, 0].set_xlabel('Document')\n",
        "axes[0, 0].set_ylabel('Word Count')\n",
        "axes[0, 0].set_title('Original vs Summary Word Count', fontsize=12, fontweight='bold')\n",
        "axes[0, 0].set_xticks(x_pos)\n",
        "axes[0, 0].set_xticklabels(range(1, len(stats_df)+1))\n",
        "axes[0, 0].legend()\n",
        "axes[0, 0].grid(axis='y', alpha=0.3)\n",
        "\n",
        "# Plot 2: Compression Ratio\n",
        "colors = plt.cm.RdYlGn_r(stats_df['Compression_Ratio'] / 100)\n",
        "axes[0, 1].barh(stats_df['Document'], stats_df['Compression_Ratio'], color=colors)\n",
        "axes[0, 1].set_xlabel('Compression Ratio (%)')\n",
        "axes[0, 1].set_title('Text Compression by Document', fontsize=12, fontweight='bold')\n",
        "axes[0, 1].grid(axis='x', alpha=0.3)\n",
        "\n",
        "# Plot 3: Summary Length Distribution\n",
        "axes[1, 0].hist(stats_df['Summary_Words'], bins=10, color='mediumseagreen',\n",
        "                edgecolor='black', alpha=0.7)\n",
        "axes[1, 0].axvline(avg_summary_length, color='red', linestyle='--',\n",
        "                   linewidth=2, label=f'Average: {avg_summary_length:.1f}')\n",
        "axes[1, 0].set_xlabel('Summary Word Count')\n",
        "axes[1, 0].set_ylabel('Frequency')\n",
        "axes[1, 0].set_title('Distribution of Summary Lengths', fontsize=12, fontweight='bold')\n",
        "axes[1, 0].legend()\n",
        "axes[1, 0].grid(axis='y', alpha=0.3)\n",
        "\n",
        "# Plot 4: Efficiency Metrics\n",
        "metrics = ['Avg Compression\\n(%)', 'Avg Summary\\nLength (words)', 'Processing\\nTime (s)']\n",
        "values = [avg_compression, avg_summary_length, 2.5]  # Sample processing time\n",
        "colors_metric = ['#FF6B6B', '#4ECDC4', '#45B7D1']\n",
        "\n",
        "axes[1, 1].bar(metrics, values, color=colors_metric, alpha=0.8, edgecolor='black')\n",
        "axes[1, 1].set_title('Summarization Performance Metrics', fontsize=12, fontweight='bold')\n",
        "axes[1, 1].set_ylabel('Value')\n",
        "axes[1, 1].grid(axis='y', alpha=0.3)\n",
        "\n",
        "for i, v in enumerate(values):\n",
        "    axes[1, 1].text(i, v + max(values)*0.02, f'{v:.1f}',\n",
        "                    ha='center', va='bottom', fontweight='bold')\n",
        "\n",
        "plt.tight_layout()\n",
        "plt.savefig('genai_summarization_analysis.png', dpi=300, bbox_inches='tight')\n",
        "print(\"\\n✓ Analysis visualizations saved as 'genai_summarization_analysis.png'\")\n"
