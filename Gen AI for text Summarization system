    {
      "cell_type": "code",
      "source": [
        "# ============================================================================\n",
        "# STEP 3: GENERATE SUMMARIES USING GEN AI\n",
        "# ============================================================================\n",
        "\n",
        "print(\"\\n[3/5] Generating AI-Powered Summaries...\")\n",
        "print(\"Using Generative AI (Transformer-based) for abstractive summarization\\n\")\n",
        "\n",
        "summaries = {}\n",
        "summary_stats = []\n",
        "\n",
        "for title, text in documents.items():\n",
        "    print(f\"Processing: {title}\")\n",
        "\n",
        "    # Clean text\n",
        "    text_clean = \" \".join(text.split())\n",
        "\n",
        "    # Generate summary using Gen AI\n",
        "    try:\n",
        "        summary = summarizer(text_clean,\n",
        "                            max_length=130,\n",
        "                            min_length=30,\n",
        "                            do_sample=False)\n",
        "\n",
        "        summary_text = summary[0]['summary_text']\n",
        "        summaries[title] = summary_text\n",
        "\n",
        "        # Calculate statistics\n",
        "        original_words = len(text_clean.split())\n",
        "        summary_words = len(summary_text.split())\n",
        "        compression_ratio = (1 - summary_words/original_words) * 100\n",
        "\n",
        "        summary_stats.append({\n",
        "            'Document': title,\n",
        "            'Original_Words': original_words,\n",
        "            'Summary_Words': summary_words,\n",
        "            'Compression_Ratio': compression_ratio\n",
        "        })\n",
        "\n",
        "        print(f\"✓ Summary generated: {summary_words} words ({compression_ratio:.1f}% compression)\")\n",
        "        print(f\"  Preview: {summary_text[:100]}...\\n\")\n",
        "\n",
        "    except Exception as e:\n",
        "        print(f\"✗ Error: {e}\\n\")\n",
        "        summaries[title] = \"Summary generation failed\""
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "NU2D31CVYA8j",
        "outputId": "7de24432-cc8d-4279-e87f-76f8386b1338"
      },
