        "This project demonstrates practical Gen AI implementation using pre-trained\n",
        "transformer models for text summarization - applicable to port documents,\n",
        "reports, and logistics communications.\n",
        "\"\"\"\n",
        "\n",
        "import numpy as np\n",
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "from transformers import pipeline, AutoTokenizer, AutoModelForSeq2SeqLM\n",
        "import warnings\n",
        "warnings.filterwarnings('ignore')\n",
        "\n",
        "print(\"=\" * 80)\n",
        "print(\"GENERATIVE AI TEXT SUMMARIZATION SYSTEM\")\n",
